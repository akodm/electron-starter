{
  "name": "electron-side",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-spring": "^9.2.3",
    "recoil": "^0.3.1",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "e-build": "yarn build && electron-builder",
    "e-start": "tsc ./public/electron.ts && concurrently \"cross-env yarn start\" \"wait-on http://localhost:3000 && electron . \"",
    "e-release": "yarn build && electron-builder --publish=always",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "autoprefixer": "^9",
    "babel-loader": "8.1.0",
    "concurrently": "^6.2.0",
    "craco-alias": "^3.0.1",
    "cross-env": "^7.0.3",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "postcss": "^7",
    "postcss-loader": "4.2.0",
    "styled-reset": "^4.3.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "wait-on": "^5.3.0"
  },
  "build": {
    "productName": "Akodm-Electron-App",
    "appId": "com.electron.akodm",
    "asar": true,
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true
    }
  },
  "author": {
    "name": "Electron-Akodm"
  }
}
